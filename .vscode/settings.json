{
	"cSpell.words": [
		"appspecific",
		"closedby",
		"commandfor",
		"Fira",
		"handandfoot",
		"lastmod",
		"picklist",
		"treeify",
		"urlset",
		"wght"
	],
	"cSpell.ignorePaths": [
		"package-lock.json",
		"node_modules",
		"vscode-extension",
		".git/{info,lfs,logs,refs,objects}/**",
		".git/{index,*refs,*HEAD}",
		".vscode",
		".vscode-insiders",
		".dev"
	],
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "always"
	},
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.tabSize": 2,
	"editor.insertSpaces": true,
	"workbench.editor.customLabels.enabled": true,
	"workbench.editor.customLabels.patterns": {
		"**/src/**/*.css": "${filename} ðŸŽ¨",
		"**/src/**/*.{js,ts}": "${filename}",
		/** SVELTE */
		"**/src/**/*.svelte": "${filename}",
		/** SVELTEKIT */
		"**/src/routes/**/+page.svelte": "${dirname(0)} (+page)",
		"**/src/routes/**/+page.{js,ts}": "${dirname(0)} (+page.${extname})",
		"**/src/routes/**/+page.server.{js,ts}": "${dirname(0)} (+page.server.${extname})",
		"**/src/routes/**/+layout.svelte": "${dirname(0)} (+layout)",
		"**/src/routes/**/+layout.{js,ts}": "${dirname(0)} (+layout.${extname})",
		"**/src/routes/**/+layout.server.{js,ts}": "${dirname(0)} (+layout.server.${extname})",
		"**/src/**/api/**/*.remote.{js,ts}**": "${filename} (Remote)",
		"**/src/hooks.client.{js,ts}": "${filename} (Client)",
		"**/src/hooks.server.{js,ts}": "${filename} (Server)"
	}
}
